{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@fortawesome/react-fontawesome": "0.1.18",
    "apexcharts": "3.35.2",
    "body-parser": "1.20.0",
    "cors": "2.8.5",
    "d3": "7.4.4",
    "eslint-config-react-app": "7.0.1",
    "express": "4.18.1",
    "multer": "1.4.4",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-normalize": "10.0.1",
    "prop-types": "15.8.1",
    "ramda": "0.28.0",
    "react": "17.0.2",
    "react-apexcharts": "1.4.0",
    "react-cookie": "4.1.1",
    "react-data-table-component": "7.5.2",
    "react-dom": "17.0.2",
    "react-redux": "8.0.1",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "react-tabs": "4.2.1",
    "redux": "4.2.0",
    "redux-actions": "2.6.5",
    "redux-mock-store": "1.5.4",
    "redux-observable": "2.0.0",
    "request": "2.88.2",
    "reselect": "4.1.5",
    "semantic-ui-react": "2.1.2",
    "serve": "13.0.2",
    "styled-components": "5.3.5",
    "uuid": "8.3.2"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "PORT=5002 nodemon --inspect=0.0.0.0:9229 server.js",
    "client": "PORT=5000 react-scripts start",
    "build": "react-scripts build",
    "test": "NODE_ENV=test NODE_PATH=./src nyc mocha src/tests/**/*.test.js*",
    "eject": "react-scripts eject",
    "start-server": "node server.js",
    "lint": "eslint --ext=js --ext=jsx ."
  },
  "proxy": "http://localhost:5002",
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-nullish-coalescing-operator"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "exclude": [
      "src/**/*.test.js*",
      "test/**"
    ],
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.17.12",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "7.17.12",
    "@babel/plugin-transform-modules-commonjs": "7.17.12",
    "@babel/preset-env": "7.17.12",
    "@babel/preset-react": "7.17.12",
    "@babel/register": "7.17.7",
    "chai": "4.3.6",
    "concurrently": "7.2.0",
    "enzyme": "3.11.0",
    "mocha": "10.0.0",
    "nodemon": "2.0.16",
    "nyc": "15.1.0",
    "typescript": "4.6.4"
  }
}
