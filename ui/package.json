{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "multer": "1.4.2",
    "prop-types": "15.7.2",
    "ramda": "0.26.1",
    "react": "16.12.0",
    "react-data-table-component": "6.0.4",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-scripts": "3.3.0",
    "react-tabs": "3.1.0",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-mock-store": "1.5.4",
    "redux-observable": "1.2.0",
    "semantic-ui-react": "0.88.2",
    "serve": "11.3.0",
    "styled-components": "4.4.1",
    "uuid": "3.3.3"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "PORT=5001 nodemon --inspect=0.0.0.0:9229 server.js",
    "client": "PORT=5000 react-scripts start",
    "build": "react-scripts build",
    "test": "NODE_ENV=test NODE_PATH=./src nyc mocha --opts ./.mocha --exit --recursive 'src/tests/*.test.js*'",
    "eject": "react-scripts eject",
    "start-server": "node server.js"
  },
  "proxy": "http://localhost:5001",
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-nullish-coalescing-operator"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "exclude": [
      "src/**/*.test.js*",
      "test/**"
    ],
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.7.4",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-react": "7.7.4",
    "@babel/register": "7.7.7",
    "chai": "4.2.0",
    "concurrently": "5.0.2",
    "enzyme": "3.11.0",
    "mocha": "6.2.2",
    "nodemon": "2.0.2",
    "nyc": "15.0.0"
  }
}
