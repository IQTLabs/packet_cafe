{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@nivo/heatmap": "0.62.0",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "d3": "6.2.0",
    "express": "4.17.1",
    "multer": "1.4.2",
    "prop-types": "15.7.2",
    "ramda": "0.27.1",
    "react": "16.14.0",
    "react-cookie": "4.0.3",
    "react-data-table-component": "6.11.5",
    "react-dom": "16.14.0",
    "react-redux": "7.2.1",
    "react-scripts": "3.4.3",
    "react-tabs": "3.1.1",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-mock-store": "1.5.4",
    "redux-observable": "1.2.0",
    "semantic-ui-react": "2.0.0",
    "serve": "11.3.2",
    "styled-components": "5.2.0",
    "uuid": "8.3.1"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "PORT=5002 nodemon --inspect=0.0.0.0:9229 server.js",
    "client": "PORT=5000 react-scripts start",
    "build": "react-scripts build",
    "test": "NODE_ENV=test NODE_PATH=./src nyc mocha src/tests/**/*.test.js*",
    "eject": "react-scripts eject",
    "start-server": "node server.js",
    "lint": "eslint --ext=js --ext=jsx ."
  },
  "proxy": "http://localhost:5002",
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-nullish-coalescing-operator"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "exclude": [
      "src/**/*.test.js*",
      "test/**"
    ],
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.12.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.12.0",
    "@babel/plugin-proposal-object-rest-spread": "7.11.0",
    "@babel/plugin-transform-modules-commonjs": "7.10.4",
    "@babel/preset-env": "7.12.0",
    "@babel/preset-react": "7.10.4",
    "@babel/register": "7.12.0",
    "chai": "4.2.0",
    "concurrently": "5.3.0",
    "enzyme": "3.11.0",
    "mocha": "8.1.3",
    "nodemon": "2.0.5",
    "nyc": "15.1.0",
    "typescript": "4.0.3"
  }
}
